@import "../../../common/vendor.wxss";

.noteDetailWrap {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.noteDetailWrap .detailHead {
  height: 151rpx;
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  background-color: #F3F3F3;
}
.noteDetailWrap .detailHead .info {
  height: 100%;
  position: absolute;
  left: 0;
  right: 160rpx;
}
.noteDetailWrap .detailHead .info .title {
  position: absolute;
  top: 40rpx;
  left: 32rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #111;
}
.noteDetailWrap .detailHead .info .time {
  position: absolute;
  bottom: 28.999999999999996rpx;
  left: 32rpx;
  font-size: 28.000000000000004rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #555;
}
.noteDetailWrap .detailHead .line {
  width: 2rpx;
  height: 60rpx;
  position: absolute;
  top: 46rpx;
  right: 158rpx;
  background-color: #ccc;
}
.noteDetailWrap .detailHead .toText {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 26rpx;
  right: 32rpx;
}
.noteDetailWrap .detailHead .toText ._i {
  width: 64rpx;
  height: 64rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  float: left;
  margin-left: 18rpx;
}
.noteDetailWrap .detailHead .toText ._span {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #555;
}
.noteDetailWrap .detailContent {
  position: absolute;
  top: 151rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0;
  overflow-y: auto;
  padding-top: 36rpx;
}
.noteDetailWrap .detailContent .noData {
  width: 543rpx;
  height: 459.99999999999994rpx;
  position: absolute;
  top: 220.00000000000003rpx;
  left: 72rpx;
}
.noteDetailWrap .detailContent .noData ._img {
  width: 543rpx;
  height: 413.99999999999994rpx;
  float: left;
}
.noteDetailWrap .detailContent .noData ._span {
  width: 100%;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  float: left;
  color: #333;
  text-align: center;
  margin-top: 8rpx;
}
.noteDetailWrap .detailContent .content {
  width: 100%;
  padding: 0 26rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.noteDetailWrap .detailContent .content ._img {
  width: 100%;
  height: 892rpx;
  float: left;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}