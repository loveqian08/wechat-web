  margin-left: 44rpx;
  margin-right: 8rpx;
  background-size: cover;
}
.chooseFile .searchWrap .search ._input {
  width: 500rpx;
  height: 100%;
  float: left;
  border: none;
  outline: none;
  font-size: 28.000000000000004rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333;
}
.chooseFile .searchWrap .isOk {
