@import "../../../common/vendor.wxss";

.chooseFile .zhezhao {
  position: fixed;
  top: 94rpx;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0,0,0,0.5);
  z-index: 4;
}
.chooseFile .imgWrap {
  width: 100%;
  position: absolute;
  margin-top: 700rpx;
  z-index: 10;
}
.chooseFile .imgWrap ._img {
  width: 100%;
}
.chooseFile .tab {
  height: 94rpx;
  background-color: #DE2910;
}
.chooseFile .tab .collection {
  width: 50%;
  float: left;
  position: relative;
}
.chooseFile .tab .collection ._span {
  height: 70rpx;
  position: absolute;
  top: 12rpx;
  left: 50%;
  line-height: 70rpx;
  transform: translateX(-50%);
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #fff;
  box-sizing: border-box;
  border-bottom: 5rpx solid #fff;
}
.chooseFile .tab .chat {
  width: 50%;
  float: right;
  position: relative;
}
.chooseFile .tab .chat ._span {
  height: 70rpx;
  position: absolute;
  top: 12rpx;
  left: 50%;
  line-height: 70rpx;
  transform: translateX(-50%);
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #fff;
}
.chooseFile .searchWrap {
  height: 104rpx;
  position: absolute;
  top: 94rpx;
  left: 32rpx;
  right: 32rpx;
  border-bottom: 1rpx solid #ddd;
  background-color: #fff;
  z-index: 1;
}
.chooseFile .searchWrap .search {
  width: 606rpx;
  height: 64rpx;
  border-radius: 32rpx;
  background-color: #EAEAEA;
  float: left;
  margin-top: 20rpx;
}
.chooseFile .searchWrap .search ._i {
  width: 28.000000000000004rpx;
  height: 28.000000000000004rpx;
  background-image: url(data:image/png;base64,);
